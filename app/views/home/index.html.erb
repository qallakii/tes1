<% if logged_in? %>
  <%= link_to "Go to Dashboard", cvs_path, class: "bg-blue-500 hover:bg-blue-700 text-white font-bold py-3 px-6 rounded" %>
<% else %>
  <%= link_to "Sign Up", signup_path, class: "bg-green-500 hover:bg-green-700 text-white font-bold py-3 px-6 rounded mr-4" %>
  <%= link_to "Login", login_path, class: "bg-blue-500 hover:bg-blue-700 text-white font-bold py-3 px-6 rounded" %>
<% end %>
